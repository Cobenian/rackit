<h2>Show slot</h2>

<ul>

  <li>
    <strong>Position:</strong>
    <%= @slot.position %>
  </li>

  <li>
    <strong>Rack:</strong>
    <%= @slot.rack.name %>
  </li>

  <li>
    <strong>Device:</strong>
    <%= if @slot.device do %>
    <%= @slot.device.name %>
    <% end %>
  </li>

</ul>

<%= link "Back", to: slot_path(@conn, :index) %>
